<?xml version="1.0" encoding="UTF-8"?>
<interface>
<template class="WidgetsEntryLoginUi" parent="GtkBox">


<child>
  <object class="GtkBox" id="formWrapper">
  <property name="orientation">vertical</property>
  <property name="spacing">15</property>
  <style>
    <class name="myWrapper"/>
  </style>


    <child>
      <object class="GtkEntry" id="usernameLogin">
        <property name="placeholder-text">Username</property>
        <attributes>
          <attribute name="weight" value="PANGO_WEIGHT_BOLD"/>
          <!-- <attribute name="background" value="red" start="5" end="10"/> -->
        </attributes>
      </object>
    </child>

    <child>
      <object class="GtkEntry" id="passwordLogin">
        <property name="placeholder-text">Password</property>
        <property name="visibility">FALSE</property>
        <attributes>
          <attribute name="weight" value="PANGO_WEIGHT_BOLD"/>
          <!-- <attribute name="background" value="red" start="5" end="10"/> -->
        </attributes>
      </object>
    </child>

    <!-- <child> -->
    <!--   <object class="GtkButton" id="changeRegister"> -->
    <!--     <property name="label">go to register</property> -->
    <!--   </object> -->
    <!-- </child> -->

  </object>
</child>


</template>
</interface>

